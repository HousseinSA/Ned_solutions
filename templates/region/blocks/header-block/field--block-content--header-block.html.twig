<div class="navbar-header">
    {% if field_name == 'field_logo' %}
    <a class="navbar-brand" href="#" {{ attributes.addClass('quickedit-field') }}>
        {{ items.0.content }}
    </a>
    {% endif %}
</div>

{% if field_name == 'field_menu_links' %}
<!-- Navigation menu -->
<div class="navigation" id="navbar" {{ attributes.addClass('quickedit-field') }}>
    <ul class="nav navbar-nav link-effect">
        {% for item in items %}
        {% if item.content['#url'] and item.content['#title'] %}
        <li>
            <a href="{{ item.content['#url'] }}">{{ item.content['#title'] }}</a>
        </li>
        {% endif %}
        {% endfor %}
    </ul>
</div>
{% endif %}


<script>
    document.querySelectorAll('.navbar-header img').forEach((img) => {
  img.classList.add('logo-img');
});
</script>